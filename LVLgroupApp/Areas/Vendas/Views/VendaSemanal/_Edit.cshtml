                                <small><span asp-validation-for="@Model.AnoAtual.LojaId" class="text-danger"></span></small>
                            </div>
                            <input type="text" id="AnoAtual_LojaId" name="AnoAtual.LojaId" asp-for="@Model.AnoAtual.LojaId" class="form-control" hidden>
                        </div>
                    </div>
