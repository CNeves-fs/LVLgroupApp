<script type="text/javascript" language=javascript>

    $("#create-form").removeData("validator");



    function GetValorTotalMensal(days) {

        let valorTotalMensal = 0;
        let countDown = days;

        if (countDown == 0) return valorTotalMensal;
        valorTotalMensal = valorTotalMensal + parseFloat(document.getElementById("AnoAtual_Valor2Feira").value);
        countDown = countDown - 1;

        if (countDown == 0) return valorTotalMensal;
        valorTotalMensal = valorTotalMensal + parseFloat(document.getElementById("AnoAtual_Valor3Feira").value);
        countDown = countDown - 1;

        if (countDown == 0) return valorTotalMensal;
        valorTotalMensal = valorTotalMensal + parseFloat(document.getElementById("AnoAtual_Valor4Feira").value);
        countDown = countDown - 1;

        if (countDown == 0) return valorTotalMensal;
        valorTotalMensal = valorTotalMensal + parseFloat(document.getElementById("AnoAtual_Valor5Feira").value);
        countDown = countDown - 1;

        if (countDown == 0) return valorTotalMensal;
        valorTotalMensal = valorTotalMensal + parseFloat(document.getElementById("AnoAtual_Valor6Feira").value);
        countDown = countDown - 1;

        if (countDown == 0) return valorTotalMensal;
        valorTotalMensal = valorTotalMensal + parseFloat(document.getElementById("AnoAtual_ValorSábado").value);
        countDown = countDown - 1;

        if (countDown == 0) return valorTotalMensal;
        valorTotalMensal = valorTotalMensal + parseFloat(document.getElementById("AnoAtual_ValorDomingo").value);
        countDown = countDown - 1;

        return valorTotalMensal;
    };



    function updateSmallBox() {

        console.log("updateSmallBox");

        let objetivosemanal = parseFloat(document.getElementById("box-objetivosemanal").innerText.replace(/[^\d.-]/g, ''));
        let objetivomensal = parseFloat(document.getElementById("box-objetivomensal").innerText.replace(/[^\d.-]/g, ''));
        let valorTotalDaVenda = parseFloat(document.getElementById("AnoAtual_ValorTotalDaVenda").value);
        let acumuladoMensal = parseFloat(document.getElementById("AnoAtual_ValorAcumuladoMensal").value);



        const currentDate = new Date();
        const currentDay = currentDate.getDate();
        let dataInicialDaSemana = document.getElementById("AnoAtual_DataInicialDaSemana").value;
        // Split the string into day, month, and year parts
        const partsDataInicialDaSemana = dataInicialDaSemana.split("/");
        const isEng = ('@localizer["pt"]' == 'en');
        // Create a new Date object by passing the year, month (0-based), and day as arguments
        let novaData = isEng ? new Date(partsDataInicialDaSemana[2], partsDataInicialDaSemana[0] - 1, partsDataInicialDaSemana[1]) : new Date(partsDataInicialDaSemana[2], partsDataInicialDaSemana[1] - 1, partsDataInicialDaSemana[0]);
        console.log("novaData = " + novaData);
        let daysInMonth = new Date(novaData.getFullYear(), novaData.getMonth() + 1, 0).getDate();
        console.log('daysInMonth = ' + daysInMonth);
        // primeiro dia da venda semanal
        let firstDay = novaData.getDate();
        // dias da venda semanal que pertencem ao mes corrente
        console.log('days to count = ' + (daysInMonth - firstDay + 1));
        let novoValorTotalMensal = 0;

        // novo valor mensal
        if (currentDay < firstDay)
        {
            novoValorTotalMensal = Math.round(valorTotalDaVenda - GetValorTotalMensal(daysInMonth - firstDay + 1));
        }
        else
        {
            novoValorTotalMensal = Math.round(acumuladoMensal + GetValorTotalMensal(daysInMonth - firstDay + 1));
        }


        $('#box-valordavenda').text(valorTotalDaVenda);
        //$('#box-valordavendamensal').text(novoValorTotalMensal);

        if (objetivosemanal > valorTotalDaVenda) {
            if ($('#smallBoxVendaSemanal').hasClass('bg-success')) {
                $('#smallBoxVendaSemanal').removeClass('bg-success')
                $('#smallBoxVendaSemanal').addClass('bg-warning')
            }
        } else {
            if ($('#smallBoxVendaSemanal').hasClass('bg-warning')) {
                $('#smallBoxVendaSemanal').removeClass('bg-warning')
                $('#smallBoxVendaSemanal').addClass('bg-success')
            }
        }

        if (objetivomensal > novoValorTotalMensal) {
            if ($('#smallBoxVendaMensal').hasClass('bg-success')) {
                $('#smallBoxVendaMensal').removeClass('bg-success')
                $('#smallBoxVendaMensal').addClass('bg-warning')
            }
        } else {
            if ($('#smallBoxVendaMensal').hasClass('bg-warning')) {
                $('#smallBoxVendaMensal').removeClass('bg-warning')
                $('#smallBoxVendaMensal').addClass('bg-success')
            }
        }

        console.log('updateSmallBox');
    };


    jQuery.validator.addMethod("loja", function (value, element) {
        //loja selecionada
        console.log("validator loja : value = " + value);
        if(value > 0) return true;
        return false;
    });

    jQuery.validator.addMethod("maiorque", function (value, element, param) {
        console.log("validator maiorque : value = " + value);
        console.log("validator maiorque : param = " + param);
        if(value > param) return true;
        return false;
    });

    $("#create-form").validate({
        rules: {
            'AnoAtual.LojaId': {
                loja: true
            },
            'AnoAtual.ValorTotalDaVenda': {
